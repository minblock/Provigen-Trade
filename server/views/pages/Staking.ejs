            transition: transform 0.3s cubic-bezier(0.2,1,0.3,1);
        }
        
        .compare-basket--active {
            -webkit-transform: translate3d(0,0,0);
            transform: translate3d(0,0,0);
        }
        
        .flexbox .compare-basket {
